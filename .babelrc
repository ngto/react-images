{
  "presets": [
    "react",
    "es2015"
  ],
  "env": {
    "test": {
      "plugins": [
        "istanbul"
      ]
    },
    "production": {
      "presets": [
        [
          "es2015",
          {
            "modules": false
          }
        ]
      ]
    }
  },
  "plugins": [
    [
      "import",
      {
        libraryName: "antd-mobile",
        style: "css"
      }
    ]
  ]
}
